
public class UmlParser {

	public static void main(String[] args) {
		System.out.println("Hello");
		
		  if (args[0].equals("class")) {
			  System.out.println(args[0]+"  "+args[0]+" "+args[0]);
	           
	        } else {
	            System.out.println("Invalid keyword " + args[0]);
	        }
	}
	
}
